>>> spec = pathspec.PathSpec.from_lines('gitwildmatch', spec.splitlines())
>>> patterns = map(pathspec.patterns.GitWildMatchPattern, spec.splitlines())
>>> spec = PathSpec(patterns)
>>> with open('patterns.list', 'r') as fh:
>>>     spec = pathspec.PathSpec.from_lines('gitwildmatch', fh)
>>> matches = spec.match_tree('path/to/python.exe')
>>> matches = spec.match_files(D:\Touhou\thcrap\repos\Touhou-Duck-Sound-Shot)
>>> is_matched = spec.match_file(D:\Touhou\thcrap\repos\Touhou-Duck-Sound-Shot)
python setup.py install
